<main class="work">
    <section>
        <h1>GRAPHIC DESIGN</h1>
        <hr>
        <div class="work-grid">
            <% graphic.each do |c|%>
            <div class="work-image">
                <img class="scroller" src='<%= c['image_url']%>'>
                <div class='work-mask'>
                    <h2><%= c['name']%></h2>
                </div>
            </div>
            <% end %>
        </div>
    </section>
    <hr>
    <section>
        <h1>WEB DESIGN</h1>
        <hr>
        <div class="work-grid">
            <% web.each do |w|%>
            <div class="work-image">
                <img src='<%= w['image_url']%>'>
                <div class='work-mask'>
                    <h2><%= w['name']%></h2>
                </div>
            </div>
            <% end %>
        </div>
    </section>
</main>